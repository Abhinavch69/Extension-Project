{
  "manifest_version": 3,
  "name": "AI Assistant",
  "version": "1.0",
  "description": "Ask anything using AI.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [],
  "host_permissions": [
    "https://js.puter.com/*"
  ]
}
